{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "photos": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "city": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::city.city",
      "inversedBy": "products"
    },
    "price": {
      "type": "biginteger",
      "required": true
    },
    "acreage": {
      "type": "decimal"
    },
    "totalBedrooms": {
      "type": "integer"
    },
    "specificAddress": {
      "type": "string"
    },
    "totalToilets": {
      "type": "integer"
    },
    "description": {
      "type": "blocks",
      "required": true
    },
    "isFeatured": {
      "type": "boolean",
      "default": false
    }
  }
}
